   "source": [
    "from scipy.stats import t\n",
    "\n",
    "# Degrees of freedom\n",
    "nu = 8\n",
    "\n",
    "# (i) P(T <= 1.860)\n",
    "p1 = t.cdf(1.860, df=nu)\n",
    "print(\"P(T <= 1.860):\", p1)\n",
    "\n",
    "# (ii) P(-2.306 <= T <= 2.306)\n",
    "p2 = t.cdf(2.306, df=nu) - t.cdf(-2.306, df=nu)\n",
    "print(\"P(-2.306 <= T <= 2.306):\", p2)\n"
   ]
  }
